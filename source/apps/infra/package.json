    "list": "cdk list"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "aws-cdk": "2.1002.0",
    "esbuild": "0.25.0"
  },
  "dependencies": {
    "@aws-smithy/server-apigateway": "1.0.0-alpha.10",
    "@aws-smithy/server-common": "1.0.0-alpha.10",
    "@aws-solutions-constructs/aws-cloudfront-s3": "^2.64.0",
    "@aws-solutions-constructs/aws-wafwebacl-apigateway": "^2.64.0",
    "@deepracer-indy/config": "workspace:*",
    "@deepracer-indy/database": "workspace:*",
    "@deepracer-indy/typescript-server-client": "workspace:*",
    "@types/aws-lambda": "^8.10.140",
    "aws-cdk-lib": "2.181.0",
    "constructs": "^10.3.0",
    "js-yaml": "^4.1.0",
