{
  "name": "@deepracer-indy/infra",
  "version": "0.0.0",
  "bin": {
    "cdk": "bin/deepRacerIndy.ts"
  },
  "scripts": {
    "build": "cdk synth",
    "clean": "rm -rf dist cdk.out",
    "cdk": "cdk",
